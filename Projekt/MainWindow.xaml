        <Button x:Name="button_atak" Content="ATAK" HorizontalAlignment="Left" Margin="58,217,0,0" VerticalAlignment="Top" Height="90" Width="200"/>
        <Button x:Name="button_heal" Content="HEAL" HorizontalAlignment="Left" Margin="520,217,0,0" VerticalAlignment="Top" Height="90" Width="200"/>
        <Button x:Name="button_def" Content="DEF" HorizontalAlignment="Left" Margin="58,325,0,0" VerticalAlignment="Top" Height="90" Width="200"/>
        <Button x:Name="button_run" Content="RUN" HorizontalAlignment="Left" Margin="520,325,0,0" VerticalAlignment="Top" Height="90" Width="200"/>





